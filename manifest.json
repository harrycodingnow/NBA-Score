{
    "manifest_version": 3,
    "name": "NBA Score",
    "version": "1.0",
    "description": "Check Latest NBA Score",
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "16.png",
        "19": "19.png",
        "38": "38.png",
        "48": "48.png",
        "128": "128.png"
      }
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ],
    "host_permissions": ["http://*/*", "https://*/*"]
  }
  